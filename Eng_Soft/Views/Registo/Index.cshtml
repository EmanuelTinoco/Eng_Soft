@model IEnumerable<Eng_Soft.Models.Registo>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Userid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data_Hora_Login)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data_Hora_Logoff)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Userid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data_Hora_Login)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data_Hora_Logoff)
            </td>
            <td>
                @Html.ActionLink("Delete", "Delete", new { id = item.ID })
            </td>
        </tr>
    }

</table>